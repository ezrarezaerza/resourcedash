{
  "manifest_version": 3,
  "name": "ResourceDash",
  "version": "1.0",
  "description": "A new tab page with simpleproductivity tools",
  "author": "Reza",
  "permissions": [
    "storage"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "chrome_url_overrides": {
    "newtab": "resourcedash.html"
  },
  "icons": {
    "16": "resourcedash_logo-16.png",
    "32": "resourcedash_logo-32.png",
    "48": "resourcedash_logo-48.png",
    "128": "resourcedash_logo-128.png"
  },
  "minimum_chrome_version": "88",
  "offline_enabled": true,
  "incognito": "split"
}